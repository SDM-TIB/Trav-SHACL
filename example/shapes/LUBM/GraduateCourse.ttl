@prefix ub: <http://swat.cse.lehigh.edu/onto/univ-bench.owl#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix : <http://example.org/> .

:GraduateCourseShape a sh:NodeShape;
